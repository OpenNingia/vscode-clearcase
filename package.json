        "key": "ctrl+j ctrl+o",
        "mac": "ctrl+j ctrl+o",
        "when": "editorTextFocus"
      },
      {
        "command": "extension.ccCheckin",
        "key": "ctrl+j ctrl+i",
        "mac": "ctrl+j ctrl+i",
        "when": "editorTextFocus"
      },
