        "vscode-clearcase.uncoKeepFile": {
          "type": "boolean",
          "default": "true",
          "markdownDescription": "Keep unchecked out files in current directory",
          "markdownDeprecationMessage": "**Breaking Change** Use new configuration: `#vscode-clearcase.cleartool.uncoKeepFile#`",
          "order": 111
        }
      }
    },
    "colors": [
      {
        "id": "ccDecoration.modifiedResourceForeground",
        "description": "%colors.modified%",
        "defaults": {
          "light": "#895503",
          "dark": "#E2C08D",
          "highContrast": "#E2C08D"
        }
      },
      {
        "id": "ccDecoration.untrackedResourceForeground",
        "description": "%colors.untracked%",
        "defaults": {
          "light": "#007100",
          "dark": "#73C991",
          "highContrast": "#73C991"
        }
      }
    ],
    "languages": [
      {
        "id": "config-spec",
        "aliases": [
          "ClearcaseConfigSpec",
          "config-spec"
        ],
        "extensions": [
          ".ics"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./Assets/icons/light/file-icon.png",
          "dark": "./Assets/icons/dark/file-icon.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "config-spec",
        "scopeName": "source.config-spec",
        "path": "./syntaxes/config-spec.tmLanguage.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/src/test/runTest.js",
    "deploy": "vsce publish",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "package": "webpack --mode production --devtool hidden-source-map"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "^12.20.55",
    "@types/tmp": "^0.2.3",
    "@types/vscode": "^1.70.0",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
